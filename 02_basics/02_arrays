const another_array = [1, 2, 3, [4, 5, 6], 7, [6, 7, [4, 5]]]

const real_another_array1 = another_array.flat(Infinity) // flattens the array to infinity depth
const real_another_array2 = another_array.flat(1) // flattens the array to 1 depth
console.log(real_another_array1);
console.log(real_another_array2);



console.log(Array.isArray("Hitesh"))
console.log(Array.from("Hitesh"))
console.log(Array.from({name: "hitesh"})) // Array.from creates an array from an iterable or array-like object 

let score1 = 100
let score2 = 200
let score3 = 300

console.log(Array.of(score1, score2, score3));